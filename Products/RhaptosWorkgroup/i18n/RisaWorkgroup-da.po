#: workgroup_delete_form.pt
#. <span i18n:translate="">Delete Workgroup:</span>
msgid "Delete Workgroup:"
msgstr "Slet arbejdsgruppe:"


#: workgroup_delete_form.pt
#. <p i18n:translate="delete_form_a">
#      You have requested that all members of this workgroup be
#      removed.  This will <strong><em>delete</em></strong> the
#      workgroup, and any changes that have been made to its
#      contents will be lost. Are you sure you want to do this?
#    </p>
msgid "delete_form_a"
msgstr "Du har bedt om at alle medlemmer af denne arbejdsgruppe bliver fjernet. Dette vil <strong><em>slette</em></strong> arbejdsgruppen og alle forandringer til dens indhold. Er du sikker på at du vil gøre dette?"

# New potential translation for new text, above?
# This will delete the workgroup, and any changes that have been made
# to its contents will be lost.
# Dette vil slette arbejdsgruppen og alle foraendringer til des inhold
# Old text, which matches the translation above was:
#   This will delete the workgroup and all of its contents.

#: workgroup_delete_form.pt
#. <input type="submit" class="destructive" name="workgroup_delete:action" value="Yes, Delete it" i18n:attributes="value"/>
msgid "Yes, Delete it"
msgstr "Ja, slet den"


#: workgroup_delete_form.pt
#. <input type="submit" class="standalone" name="workgroup_members:action" value="No, Cancel" i18n:attributes="value"/>
#: exit_form.pt
#. <input type="submit" class="standalone" name="workgroup_members:action" value="No, Cancel" i18n:attributes="value"/>
msgid "No, Cancel"
msgstr "Nej, aflys"


#: exit_form.pt
#. <span i18n:translate="">Leave Workgroup:</span>
msgid "Leave Workgroup:"
msgstr "Forlad arbejdsgruppe:"


#: exit_form.pt
#. <p i18n:translate="exit_form_a">
#      You are about to leave this workgroup.  <strong><em>You will not be
#      able to rejoin</em></strong> this group unless added by another group
#      member. Are you sure you want to do this?
#    </p>
msgid "exit_form_a"
msgstr "Du er ved at forlade denne arbejdsgruppe. <strong><em>Du vil ikke have mulighed for at gen-tilslutte dig</em></strong> denne gruppe med mindre du bliver tilføjet af et andet gruppemedlem. Er du sikker på at du vil gøre dette?"


#: exit_form.pt
#. <input type="submit" class="destructive" name="workgroup_exit:action" value="Yes, Leave" i18n:attributes="value"/>
msgid "Yes, Leave"
msgstr "Ja, forlad"


